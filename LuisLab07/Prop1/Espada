public class Espada extends Item{
    private int ataque;
    private int alcance;

    public Espada() {
        super("Espada", "Tier 0. Ataque alto y Alcance bajo.");
        this.ataque = 10;
        this.alcance = 2;
    }

    public int getAtaque() { return ataque; }
    public int getAlcance() { return alcance; }
}
