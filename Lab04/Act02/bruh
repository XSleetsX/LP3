package Lab04;
import java.util.*;

public class Banco {
	private List<CuentaBancaria> cuentas;
	private String nombre;
	
	public Banco(CuentaBancaria[] cuentas, String nombre) {
		this.cuentas = cuentas;
		this.nombre = nombre;
	}
	
	public void agregarCuenta(CuentaBancaria nueva) {
		for (CuentaBancaria cuenta : cuentas) {
			if (cuenta.getNumeroCuenta() == nueva.getNumeroCuenta()) {
				
			}
		}
		cuentas.
	}
}
